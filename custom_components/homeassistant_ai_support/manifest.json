{
    "domain": "homeassistant_ai_support",
    "name": "Wsparcie AI Home Assistant",
    "documentation": "https://github.com/smartkwadrat/homeassistant-ai-support",
    "dependencies": ["http", "recorder"],
    "codeowners": ["@smartkwadrat"],
    "requirements": ["openai>=1.0.0"],
    "config_flow": true,
    "iot_class": "cloud_polling",
    "version": "0.1.0",
    "issue_tracker": "https://github.com/smartkwadrat/homeassistant-ai-support/issues",
    "loggers": ["homeassistant_ai_support"],
    "translations": [
      {
        "language": "en",
        "path": "translations/en.json"
      },
      {
        "language": "pl",
        "path": "translations/pl.json"
      }
    ]
  }
